<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>dashboard</title>
  <link rel="stylesheet" href="./notes.css">
</head>

<body>
  <div class="dashboard-container">
    <%- include('./Reused/dashBoardHeader') %>
      <div class="dashboard-mid">
        <div class="dashboard-second">
          <h1>Hey , John</h1>
          <a class="btn new-note-button" href="/create">+ New Note</a>
        </div>
        <a href="/update" class="card-container">
          <% NotesData.forEach(element=> { %>
            <div class="card-data">
              <h4>
                <%= element.title %>
              </h4>
              <p>
                <%= element.description %>
              </p>
            </div>
            <% }); %>
        </a>
      </div>
  </div>
</body>

</html>